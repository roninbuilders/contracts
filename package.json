{
  "name": "@roninbuilders/contracts",
  "version": "0.3.1",
  "description": "Ronin Network Contracts",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch; tsc -w",
    "build": "tsup",
    "update-contracts": "node --loader ts-node/esm ./src/update.ts",
    "format": "pnpm dlx @biomejs/biome format ./src --write",
    "upload": "pnpm run build && npm publish --yes",
    "clean": "rm -rf .turbo && rm -rf ./dist"
  },
  "type": "module",
  "author": [
    "Ronin Builders <3",
    "Glitch-txs",
    "alexx855"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "keywords": [
    "contracts",
    "ronin",
    "defi",
    "network",
    "web3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roninbuilders/contracts.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@types/node": "^20.11.10",
    "abitype": "^0.10.3",
    "ts-node": "^10.9.2",
    "tsup": "^6.7.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.5.1"
}
